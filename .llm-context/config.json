{
  "profiles": {
    "code": {
      "name": "code",
      "gitignores": {
        "full_files": [
          ".git/",
          "node_modules/",
          "dist/",
          "build/",
          ".env",
          ".env.*",
          "*.min.*",
          "*.bundle.*"
        ],
        "outline_files": [
          ".git/",
          "node_modules/",
          "dist/",
          "build/",
          ".env",
          ".env.*",
          "*.min.*",
          "*.bundle.*"
        ]
      },
      "only_includes": {
        "full_files": [
          "**/*"
        ],
        "outline_files": [
          "**/*"
        ]
      },
      "settings": {
        "no_media": true,
        "with_user_notes": false
      }
    },
    "code-prompt": {
      "name": "code-prompt",
      "gitignores": {
        "full_files": [
          ".git/",
          "node_modules/",
          "dist/",
          "build/",
          ".env",
          ".env.*",
          "*.min.*",
          "*.bundle.*"
        ],
        "outline_files": [
          ".git/",
          "node_modules/",
          "dist/",
          "build/",
          ".env",
          ".env.*",
          "*.min.*",
          "*.bundle.*"
        ]
      },
      "only_includes": {
        "full_files": [
          "**/*"
        ],
        "outline_files": [
          "**/*"
        ]
      },
      "settings": {
        "no_media": true,
        "with_user_notes": false
      },
      "prompt": "prompt.md"
    }
  },
  "default_profile": "code"
}